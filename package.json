{
	"name": "proj-h",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"init-db": "node init-db.js",
		"db-reset": "wrangler d1 execute english-learn --file=./schema.sql --local",
		"test:unit": "tsx -r tsconfig-paths/register tests/run.ts | tap-spec",
		"coverage": "c8 -r text -r lcov npm run -s test:unit",
		"ci": "npm run -s coverage"
	},
	"dependencies": {
		"@ant-design/icons": "^6.0.0",
		"@opennextjs/cloudflare": "^1.6.5",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.10",
		"antd": "^5.26.7",
		"bcryptjs": "^3.0.2",
		"dayjs": "^1.11.13",
		"jsonwebtoken": "^9.0.2",
		"next": "15.3.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.9",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/tape": "^5.8.1",
		"c8": "^10.1.3",
		"tailwindcss": "^4",
		"tap-spec": "^5.0.0",
		"tape": "^5.9.0",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.20.4",
		"typescript": "^5",
		"wrangler": "^4.27.0"
	}
}
